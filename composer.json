{
    "name": "stevelacey/laravel-browsable-api",
    "description": "Browsable API for Laravel, based on Django REST Framework's Browsable API",
    "keywords": ["laravel", "browsable", "api", "drf", "rest", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Lacey",
            "email": "steve@stevelacey.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "*"
    },
    "suggest": {
        "barryvdh/laravel-debugbar": "Integrates PHP Debug Bar",
        "beyondcode/laravel-query-detector": "Notifies you when you should add eager loading (N+1 queries)"
    },
    "autoload": {
        "psr-4": {
            "Steve\\LaravelBrowsableApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Steve\\LaravelBrowsableApi\\ServiceProvider"
            ]
        }
    }
}
